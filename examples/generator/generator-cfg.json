{
  "metadata":{
    "name":"openshift-generator"
  },
  "kind":"Config",
  "apiVersion":"v1beta1",
  "items":[
    {
      "apiVersion":"v1beta2",
      "creationTimestamp":null,
      "id":"openshift-generator",
      "kind":"Service",
      "port":8001,
      "containerPort":8080,
      "selector":{
        "name":"generatorpod"
      }
    },
    {
      "metadata":{
        "name":"openshift-generator",
      },
      "kind":"DeploymentConfig",
      "apiVersion":"v1beta1",
      "triggers":[
        {
          "type":"ConfigChange",
        }
      ],
      "template":{
        "strategy":{
          "type":"Recreate"
        },
        "controllerTemplate":{
          "replicas":1,
          "replicaSelector":{
            "name":"generatorpod"
          },
          "podTemplate":{
            "desiredState":{
              "manifest":{
                "containers":[
                  {
                    "image":"openshift/generator",
                    "imagePullPolicy":"PullIfNotPresent",
                    "name":"generator-container",
                    "ports":[
                      {
                        "containerPort":8080,
                        "protocol":"TCP"
                      }
                    ]
                  }
                ],
                "version":"v1beta1"
              },
              "restartpolicy":{

              }
            },
            "labels":{
              "name":"generatorpod"
            }
          }
        }
      }
    }
  ]
}
